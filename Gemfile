source 'https://rubygems.org'

# Bundle edge Rails instead: gem 'rails', github: 'rails/rails'
gem 'rails', '4.0.0'

# Use sqlite3 as the database for Active Record
gem 'mysql'


group :assets do
	gem 'sass-rails', '~> 4.0.0.rc2'
    gem 'coffee-rails', '~> 4.0.0'

    gem 'uglifier', '>= 1.3.0'
end

gem 'jquery-rails'

group :production do
	gem 'pg', '0.12.2'
end